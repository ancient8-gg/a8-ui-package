{
  "name": "@ancient8/components",
  "version": "2.2.3",
  "author": "ancient8",
  "description": "Ancient8 Components",
  "license": "MIT",
  "files": [
    "dist"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/ancient8-gg/a8-ui-package"
  },
  "sideEffects": false,
  "exports": {
    ".": {
      "import": "./dist/esm/index.js",
      "require": "./dist/cjs/index.js"
    },
    "./styles.scss": {
      "default": "./dist/styles/index.scss"
    }
  },
  "typesVersions": {
    "*": {
      "index": [
        "src/index"
      ]
    }
  },
  "publishConfig": {
    "access": "public",
    "provenance": true,
    "typesVersions": {
      "*": {
        "index": [
          "dist/esm/index.d.ts"
        ]
      }
    }
  },
  "scripts": {
    "clean": "rimraf .turbo",
    "build:esm": "swc src -d dist/esm -D --ignore src/styles --strip-leading-paths --config-file ./.swcrc.esm.json",
    "build:cjs": "swc src -d dist/cjs -D --ignore src/styles --strip-leading-paths --config-file ./.swcrc.cjs.json",
    "build:types-esm": "tsc -p tsconfig.build.json --emitDeclarationOnly --noCheck --outDir dist/esm",
    "build:types-cjs": "tsc -p tsconfig.build.json --emitDeclarationOnly --noCheck --outDir dist/cjs",
    "build:alias-esm": "tsc-alias --outDir dist/esm",
    "build:alias-cjs": "tsc-alias --outDir dist/cjs",
    "build:style-extract": "swc src/styles -d dist -D --ignore src/styles/tailwind.scss --strip-leading-paths",
    "build:style": "pnpm build:style-extract && tailwindcss -i src/styles/tailwind.scss -o ./dist/styles/tailwind.scss --postcss postcss.config.mjs",
    "dev:style": "tailwindcss -i src/styles/tailwind.scss -o ./dist/styles/tailwind.scss --watch",
    "dev": "concurrently \"pnpm build:esm -w\" \"pnpm build:types-esm -w\" \"pnpm build:alias-esm -w\" \"pnpm build:style-extract -w\" \"pnpm dev:style\"",
    "predev": "rimraf ./dist",
    "build": "pnpm run \"/^build:.*/\" ",
    "prebuild": "rimraf ./dist",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:ui": "vitest --ui"
  },
  "dependencies": {
    "@rainbow-me/rainbowkit": "^2.2.0",
    "@tanstack/react-query": "^5.0.0",
    "antd": "^5.21.6",
    "clsx": "^2.1.1",
    "copy-to-clipboard": "^3.3.3",
    "react": "^18.0.0",
    "react-dom": "^18.0.0",
    "viem": "^2.0.0",
    "wagmi": "^2.0.0",
    "zustand": "^5.0.0"
  },
  "devDependencies": {
    "@ant-design/icons": "^5.5.2",
    "@swc/cli": "^0.4.0",
    "@swc/core": "^1.7.36",
    "@swc/helpers": "^0.5.13",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.2.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/node": "^20.16.0",
    "@types/react": "^18.3.11",
    "@vitejs/plugin-react": "^4.3.2",
    "@vitest/ui": "^3.1.1",
    "axios": "^1.7.7",
    "chokidar": "^4.0.1",
    "concurrently": "^9.1.2",
    "iconsax-react": "^0.0.8",
    "jsdom": "^26.0.0",
    "npm-run-all": "^4.1.5",
    "react-use": "^17.5.1",
    "rimraf": "^6.0.1",
    "sass": "^1.80.3",
    "tailwindcss": "^3.4.17",
    "tsc-alias": "^1.8.10",
    "typescript": "^5.6.3",
    "vitest": "^3.1.1"
  }
}
