{
  "name": "@ancient8/components",
  "version": "0.1.1",
  "author": "ancient8",
  "description": "Ancient8 Components",
  "license": "MIT",
  "type": "module",
  "files": [
    "dist/**/*.{js,d.ts,[s]css,woff2,svg}"
  ],
  "main": "./dist/index.js",
  "sideEffects": false,
  "exports": {
    ".": {
      "default": "./dist/index.js"
    },
    "./styles.scss": {
      "default": "./dist/styles/index.scss"
    }
  },
  "typesVersions": {
    "*": {
      "index": [
        "src/index"
      ]
    }
  },
  "publishConfig": {
    "access": "public",
    "typesVersions": {
      "*": {
        "index": [
          "dist/index.d.ts"
        ]
      }
    }
  },
  "scripts": {
    "clean": "rimraf ./dist; rimraf ./.turbo;",
    "build": "pnpm build:swc && pnpm build:types && pnpm build:alias",
    "prebuild": "rimraf ./dist; rimraf ./tsconfig.tsbuildinfo",
    "build:swc": "swc src -d dist --copy-files --strip-leading-paths",
    "build:alias": "tsc-alias",
    "build:types": "tsc --emitDeclarationOnly",
    "dev": "run-p \"build:swc -w\" \"build:types -w\" \"build:alias -w\"",
    "predev": "rimraf ./dist"
  },
  "peerDependencies": {
    "@rainbow-me/rainbowkit": ">= 2.2.0",
    "@tanstack/react-query": ">= 5.0.0",
    "react": ">= 18.0.0",
    "react-dom": ">= 18.0.0",
    "viem": ">= 2.0.0",
    "wagmi": ">= 2.0.0",
    "zustand": ">= 5.0.0"
  },
  "dependencies": {
    "antd": "^5.21.6",
    "clsx": "^2.1.1"
  },
  "devDependencies": {
    "@ant-design/static-style-extract": "^1.0.3",
    "@swc/cli": "^0.4.0",
    "@swc/core": "^1.7.36",
    "@swc/helpers": "^0.5.13",
    "@types/node": "^20.16.0",
    "@types/react": "^18.3.11",
    "axios": "^1.7.7",
    "chokidar": "^4.0.1",
    "iconsax-react": "^0.0.8",
    "npm-run-all": "^4.1.5",
    "react-use": "^17.5.1",
    "rimraf": "^6.0.1",
    "sass": "^1.80.3",
    "tsc-alias": "^1.8.10",
    "tsx": "^4.19.2",
    "typescript": "^5.6.3"
  }
}
